        <jsScript_type>0</jsScript_type>
        <jsScript_name>StringReplacement</jsScript_name>
        <jsScript_script>// String Replacement

function isValid(str) {
	return str.length > 0;
}


if (isValid(condutor)) {
	var condutor = condutor
		.replace(/^S(IM)?$/, "Sim")
		.replace(/^0$/, "Não informado")
		.replace(/\bN(ÃO)?\b/, "Não")
		.replace(/\bINFORMADO\b/, "informado");
} else {
	var condutor = "Não informado";
}



if (isValid(severidade_desc)) {
	var severidade_desc = severidade_desc
		.replace(/\bNAO\b/, "Não")
		.replace(/\bINFORMADO\b/, "informado")
		.replace(/^FATAL\b/, "Fatal")
		.replace(/ FATAL\b/, " fatal")
		.replace(/^SEM FERIMENTOS$/, "Sem ferimentos");
} else {
    var severidade_desc = "Não informado";
}



if (isValid(sexo)) {
	var sexo = sexo
		.replace(/^0$/, "Não informado")
		.replace(/^M$/, "Masculino")
		.replace(/^F$/, "Feminino");
} else {
	var sexo = "Não informado";
}



if (isValid(cinto_seguranca)) {
	var cinto_seguranca = cinto_seguranca
		.replace(/^SIM$/, "Sim")
		.replace(/\bNÃO\b/, "Não")
		.replace(/\bINFORMADO\b/, "informado");
} else {
	var cinto_seguranca = "Não informado";
}



if (isValid(embreagues)) {
	var embreagues = embreagues
		.replace(/^SIM$/, "Sim")
		.replace(/\bNÃO\b/, "Não")
		.replace(/\bINFORMADO\b/, "informado");
} else {
	var embreagues = "Não informado";
}



if (isValid(habilitacao_cod)) {
	var habilitacao_cod = habilitacao_cod.replace(/^(|N)$/, "NI");
} else {
    var habilitacao_cod = "NI";
}



if (isValid(habilitacao_desc)) {
	var habilitacao_desc = habilitacao_desc.charAt(0).toUpperCase() +
								habilitacao_desc.slice(1).toLowerCase();

	var habilitacao_desc = habilitacao_desc
		.replace(/\b([Vv])eic(\.|ulos?\b)/, "$1eíc$2")
		.replace(/\b([Nn])ao\b/, "$1ão")
		.replace(/\b([Mm])ax\./, "$1áx.")
		.replace(/\b([Mm])aximo/, "$1áximo");
} else {
	var habilitacao_desc = "Não informado";
}


</jsScript_script>
      </jsScript>
