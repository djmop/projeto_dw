function log10(x) {
	return Math.log(x) / Math.LN10;
}

function coordinateMagnitudeFix(coordinate, validMagnitude) {
	if (coordinate != null) {
		var magnitude = log10(coordinate);
		magnitude = Math.floor(magnitude);

		var difference = magnitude - validMagnitude;

		if (difference > 0)
			return coordinate / Math.pow(10, difference);
	}
	return coordinate;
}

coordenada_y = coordinateMagnitudeFix(coordenada_y, 6);
coordenada_x = coordinateMagnitudeFix(coordenada_x, 5);


