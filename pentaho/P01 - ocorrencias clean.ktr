        <jsScript_script>//Script here

function getTimeOfDay(t) {
	if (t >=  0 &amp;&amp; t &lt;  1) return "00h à  01h";
	if (t >=  1 &amp;&amp; t &lt;  2) return "01h às 02h";
	if (t >=  2 &amp;&amp; t &lt;  3) return "02h às 03h";
	if (t >=  3 &amp;&amp; t &lt;  4) return "03h às 04h";
	if (t >=  4 &amp;&amp; t &lt;  5) return "04h às 05h";
	if (t >=  5 &amp;&amp; t &lt;  6) return "05h às 06h";
	if (t >=  6 &amp;&amp; t &lt;  7) return "06h às 07h";
	if (t >=  7 &amp;&amp; t &lt;  8) return "07h às 08h";
	if (t >=  8 &amp;&amp; t &lt;  9) return "08h às 09h";
	if (t >=  9 &amp;&amp; t &lt; 10) return "09h às 10h";
	if (t >= 10 &amp;&amp; t &lt; 11) return "10h às 11h";
	if (t >= 11 &amp;&amp; t &lt; 12) return "11h às 12h";
	if (t >= 12 &amp;&amp; t &lt; 13) return "12h às 13h";
	if (t >= 13 &amp;&amp; t &lt; 14) return "13h às 14h";
	if (t >= 14 &amp;&amp; t &lt; 15) return "14h às 15h";
	if (t >= 15 &amp;&amp; t &lt; 16) return "15h às 16h";
	if (t >= 16 &amp;&amp; t &lt; 17) return "16h às 17h";
	if (t >= 17 &amp;&amp; t &lt; 18) return "17h às 18h";
	if (t >= 18 &amp;&amp; t &lt; 19) return "18h às 19h";
	if (t >= 19 &amp;&amp; t &lt; 20) return "19h às 20h";
	if (t >= 20 &amp;&amp; t &lt; 21) return "20h às 21h";
	if (t >= 21 &amp;&amp; t &lt; 22) return "21h às 22h";
	if (t >= 22 &amp;&amp; t &lt; 23) return "22h às 23h";
	if (t >= 23 &amp;&amp; t &lt; 24) return "23h às 24h";
	else return null;
}


var hh = boletim_datahora.getHours();
var mm = boletim_datahora.getMinutes();

var t = hh + mm/60;

var intervalo_horario = getTimeOfDay(t);


</jsScript_script>
