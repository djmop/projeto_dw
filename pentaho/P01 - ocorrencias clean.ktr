      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>import org.geotools.referencing.CRS;
import org.geotools.geometry.jts.JTS;
import org.locationtech.jts.geom.Coordinate;
import org.opengis.referencing.FactoryException;
import org.opengis.referencing.crs.CoordinateReferenceSystem;
import org.opengis.referencing.operation.MathTransform;
import org.opengis.referencing.operation.TransformException;

String coordXField;
String coordYField;
String latitudeField;
String longitudeField;

public boolean coordinateIsValid(Double coordinate) {
  return coordinate != null &amp;&amp; coordinate != 0;
}

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi)
throws KettleException, FactoryException, TransformException
{
  // First, get a row from the default input hop
  Object[] r = getRow();

  // If the row object is null, we are done processing.
  if (r == null) {
     setOutputDone();
     return false;
  }

  // Let's look up parameters only once for performance reason.
  if (first) {
     coordXField = getParameter("COORDENADA_X");
     coordYField = getParameter("COORDENADA_Y");
     latitudeField  = getParameter("LATITUDE");
     longitudeField = getParameter("LONGITUDE");
     first = false;
  }

  // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
  // enough to handle any new fields you are creating in this step.
  Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());

  // Getting field values
  Double x = get(Fields.In, coordXField).getNumber(r);
  Double y = get(Fields.In, coordYField).getNumber(r);

  Double latitude  = null;
  Double longitude = null;


  // Converting Coordinates
  if ( coordinateIsValid(x) &amp;&amp; coordinateIsValid(y) ) {
	 CoordinateReferenceSystem sourceCRS = CRS.decode("EPSG:31983");
     CoordinateReferenceSystem targetCRS = CRS.decode("EPSG:4326");

     MathTransform transform = CRS.findMathTransform(sourceCRS, targetCRS);

     Coordinate sourceCoord = new Coordinate(x, y);
     Coordinate targetCoord = JTS.transform(sourceCoord, sourceCoord, transform);

     latitude  = targetCoord.getX();
     longitude = targetCoord.getY();
  }

  get(Fields.Out, latitudeField).setValue(outputRow, latitude);
  get(Fields.Out, longitudeField).setValue(outputRow, longitude);

  // putRow will send the row on to the default output hop.
  putRow(data.outputRowMeta, outputRow);

  return true;
}</class_source>
