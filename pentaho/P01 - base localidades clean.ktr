function replaceDE(str, patterns) {
   for (var i=0; i &lt; patterns.length; i++) {
      var pat = new RegExp("(" + patterns[i] + ")d([aeo]s?)\\b");
      str = str.replace(pat, "$1 d$2");
   }
   return str;
}


