    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>
if (data != null) {
	var boletimAno = year(data);
	var boletimMes = month(data);
	var boletimDia = getDayNumber(data, 'm');
	var boletimHora = data.getHours();

	var boletimDiaSemana = getDayNumber(data, 'w');

	var boletimDiaSemanaDesc = '';

	switch(boletimDiaSemana) {
		case 1:
			boletimDiaSemanaDesc = 'Domingo';
			break;
		case 2:
			boletimDiaSemanaDesc = 'Segunda-feira';
			break;
		case 3:
			boletimDiaSemanaDesc = 'Terça-feira';
			break;
		case 4:
			boletimDiaSemanaDesc = 'Quarta-feira';
			break;
		case 5:
			boletimDiaSemanaDesc = 'Quinta-feira';
			break;
		case 6:
			boletimDiaSemanaDesc = 'Sexta-feira';
			break;
		case 7:
			boletimDiaSemanaDesc = 'Sábado';
			break;
	}


	var boletimMesDesc = '';

	switch (boletimMes) {
		case 1:
			boletimMesDesc = "Janeiro";
			break;
		case 2:
			boletimMesDesc = "Fevereiro";
			break;
		case 3:
			boletimMesDesc = "Março";
			break;
		case 4:
			boletimMesDesc = "Abril";
			break;
		case 5:
			boletimMesDesc = "Maio";
			break;
		case 6:
			boletimMesDesc = "Junho";
			break;
		case 7:
			boletimMesDesc = "Julho";
			break;
		case 8:
			boletimMesDesc = "Agosto";
			break;
		case 9:
			boletimMesDesc = "Setembro";
			break;
		case 10:
			boletimMesDesc = "Outubro";
			break;
		case 11:
			boletimMesDesc = "Novembro";
			break;
		case 12:
			boletimMesDesc = "Dezembro";
			break;
	}


	var dataTempoKey = num2str(boletimAno);

	if (boletimMes &lt; 10)
		dataTempoKey += '0' + num2str(boletimMes);
	else
		dataTempoKey += num2str(boletimMes);

	if (boletimDia &lt; 10)
		dataTempoKey += '0' + num2str(boletimDia);
	else
		dataTempoKey += num2str(boletimDia);

	if (boletimHora &lt; 10)
		dataTempoKey += '0' + num2str(boletimHora);
	else
		dataTempoKey += num2str(boletimHora);

	dataTempoKey = str2num(dataTempoKey);
}</jsScript_script>
      </jsScript>
      <jsScript>
        <jsScript_type>-1</jsScript_type>
        <jsScript_name>getDayNumber_Sample</jsScript_name>
        <jsScript_script>// Returns the number of a date, inside the
// given type.
//
// Usage:
// getDayNumber(var,var);
// 1: Date - The Variable with the Date.
// 2: String - The Type:
// y - Day in Year
// m - Day in Month
// w - Day in Week
// wm - Day of Week in Month 
//
// 2006-11-15
//
var d1 = new Date();
Alert(getDayNumber(d1, "y"));
Alert(getDayNumber(d1, "m"));
Alert(getDayNumber(d1, "w"));
Alert(getDayNumber(d1, "wm"));
      </jsScript_script>
      </jsScript>
